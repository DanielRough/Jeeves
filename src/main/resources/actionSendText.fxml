<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<Pane fx:id="pane" maxHeight="30.0" minHeight="30.0" pickOnBounds="false" prefHeight="30.0" styleClass="action" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Pane layoutX="-14.0" layoutY="9.0" prefHeight="12.0" prefWidth="15.0" styleClass="action_join" />
      <HBox fx:id="hboxSMS" alignment="CENTER_LEFT" fillHeight="false" layoutX="1.0" layoutY="2.0" maxHeight="26.0" prefHeight="26.0" spacing="5.0">
         <children>
            <Label text="send SMS: ">
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
               <HBox.margin>
                  <Insets left="5.0" />
               </HBox.margin>
            </Label>
            <TextField fx:id="txtMessage" maxHeight="26.0" minWidth="-Infinity" prefHeight="26.0" prefWidth="49.0" promptText="SMS" />
            <Label alignment="CENTER" layoutX="212.0" text="to">
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
               <padding>
                  <Insets left="2.0" right="2.0" />
               </padding>
            </Label>
         </children>
      </HBox>
      <TextArea fx:id="smsText" layoutX="125.0" layoutY="3.0" prefHeight="163.0" prefWidth="104.0" stylesheets="@Styles.css" visible="false" />
      <ComboBox fx:id="cboRecipient" layoutX="152.0" layoutY="5.0" prefHeight="15.0" stylesheets="@Styles.css">
         <styleClass>
            <String fx:value="styled-select" />
            <String fx:value="shadowy" />
         </styleClass>
      </ComboBox>
   </children>
   <stylesheets>
      <URL value="@Styles.css" />
   </stylesheets>
</Pane>
