                           </padding>
                        </Button>
                        <Button fx:id="btnAddTime" layoutX="66.0" layoutY="13.0" mnemonicParsing="false" onAction="#handleAddTime" prefWidth="26.0" text="+" textAlignment="CENTER" wrapText="true">
