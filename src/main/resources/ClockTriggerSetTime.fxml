                     <children>
                        <Button fx:id="btnRemoveTime" layoutX="66.0" layoutY="43.0" mnemonicParsing="false" onAction="#handleRemoveTime" prefWidth="26.0" text="-" textAlignment="CENTER" wrapText="true">
