<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="506.0" prefWidth="287.0" spacing="5.0" style="-fx-border-color: #D3D3D3;" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Pane prefHeight="273.0" prefWidth="287.0">
         <children>
            <VBox fx:id="vboxOpts" layoutX="-1.0" prefHeight="201.0" prefWidth="276.0">
               <children>
                  <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="28.0" prefWidth="280.0" text="Edit Question" textAlignment="CENTER">
                     <font>
                        <Font size="22.0" />
                     </font>
                  </Label>
                  <Label text="Question Text">
                     <font>
                        <Font size="16.0" />
                     </font>
                     <VBox.margin>
                        <Insets top="5.0" />
                     </VBox.margin>
                  </Label>
                  <TextField fx:id="txtQText" prefHeight="35.0" prefWidth="258.0" promptText="How are you?">
                     <font>
                        <Font size="18.0" />
                     </font>
                     <VBox.margin>
                        <Insets bottom="20.0" />
                     </VBox.margin>
                  </TextField>
               </children>
               <padding>
                  <Insets top="10.0" />
               </padding>
            </VBox>
         </children>
         <opaqueInsets>
            <Insets bottom="2.0" left="2.0" right="2.0" top="2.0" />
         </opaqueInsets>
      </Pane>
      <HBox fx:id="hboxCondition" alignment="CENTER_LEFT">
         <children>
            <CheckBox fx:id="chkAskOnCondition" mnemonicParsing="false" prefHeight="36.0" prefWidth="177.0" text="Ask this if answer to:" wrapText="true">
               <font>
                  <Font size="14.0" />
               </font>
            </CheckBox>
            <ImageView fx:id="imgCondition" fitHeight="14.0" fitWidth="14.0" pickOnBounds="true" preserveRatio="true" visible="false">
               <image>
                  <Image url="@information-icon.png" />
               </image>
            </ImageView>
         </children>
      </HBox>
      <Pane fx:id="paneCondition" prefHeight="58.0" prefWidth="287.0">
         <children>
            <Label layoutX="240.0" layoutY="-10.0" prefHeight="25.0" prefWidth="33.0" text="was">
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <TextField fx:id="txtNumAnswer" layoutX="135.0" layoutY="25.0" prefHeight="25.0" prefWidth="42.0" visible="false" />
            <TextField fx:id="txtAnswer" layoutY="25.0" prefHeight="25.0" />
            <ComboBox fx:id="cboQuestionText" layoutY="-10.0" prefHeight="25.0" prefWidth="229.0" />
            <RadioButton fx:id="rdioTrue" layoutX="7.0" layoutY="25.0" mnemonicParsing="false" text="true" visible="false" />
            <RadioButton fx:id="rdioFalse" layoutX="62.0" layoutY="25.0" mnemonicParsing="false" text="false" visible="false" />
            <ComboBox fx:id="cboLessMore" layoutX="-1.0" layoutY="25.0" prefHeight="25.0" prefWidth="135.0" visible="false" />
            <ComboBox fx:id="cboMultiChoice" layoutX="-1.0" layoutY="25.0" prefWidth="150.0" visible="false" />
         </children>
      </Pane>
      <HBox fx:id="hboxSaveAs" alignment="CENTER_LEFT">
         <children>
            <CheckBox fx:id="chkAssignToVar" mnemonicParsing="false" prefHeight="33.0" prefWidth="177.0" text="Save user's answer as:" wrapText="true">
               <font>
                  <Font size="14.0" />
               </font>
            </CheckBox>
            <ImageView fx:id="imgSaveAs" fitHeight="14.0" fitWidth="14.0" pickOnBounds="true" preserveRatio="true" visible="false">
               <image>
                  <Image url="@information-icon.png" />
               </image>
            </ImageView>
         </children>
      </HBox>
      <Pane fx:id="paneAssignToVar" prefHeight="72.0" prefWidth="287.0">
         <children>
            <ComboBox fx:id="cboVars" layoutX="30.0" layoutY="2.0" prefWidth="150.0" />
         </children>
      </Pane>
   </children>
   <padding>
      <Insets left="5.0" />
   </padding>
</VBox>
